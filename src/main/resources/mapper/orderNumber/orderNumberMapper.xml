<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.appleshopingmall.order.OrderNumber.OrderNumberMapper">
    <resultMap id="orderEntity" type="com.appleshopingmall.order.OrderNumber.OrderNumberEntity">
        <result property="orderNumberId" column="order_group_id"/>
        <result property="memberId" column="member_id"/>
        <result property="orderDate" column="order_date"/>
    </resultMap>

    <select id="findByMemberId" resultMap="orderEntity">
        select * from order_create_number where member_id = #{memberId} order by order_date desc
    </select>
</mapper>